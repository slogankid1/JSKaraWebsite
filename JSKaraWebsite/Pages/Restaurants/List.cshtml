@page
@model ListModel
@{
}
<h1>Restaurants</h1>

<table class="table">
    <tr>
        <td class="h3">Name</td>
        <td class="h3">CuisineType</td>
        <td class="h3">Location</td>
    </tr>
    @foreach (var restaurant in Model.Restaurants)
    {
        <tr>
            <td>@restaurant.Name</td>
            <td>@restaurant.cuisineType</td>
            <td>@restaurant.Location</td>
        </tr>
    }
</table>

<div></div>